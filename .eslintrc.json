{
    "extends": [
        "react-app",
        //"plugin:prettier/recommended",
        "plugin:import/errors",
        "eslint:recommended",
        "prettier"
    ],
    "rules": {
        "react/prop-types": 0,
        "quotes": ["error", "double"],
        "no-unused-vars": "warn",
        "prettier/prettier": "error"
    },
    "plugins": ["prettier"],
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
