{
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "plugin:import/errors",
    "eslint:recommended"
  ],
  "rules": {
    "react/prop-types": 0,
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "no-unused-vars": "warn"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
